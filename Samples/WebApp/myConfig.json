{
  "modules": [
    {
      "type": "WebApp.MyModule, WebApp",
      "parameters": {
        "configuration": {
          "myStringProperty": "some string value"
        }
      }
    },
    {
      "type": "Baubit.Logging.Telemetry.DI.Module, Baubit",
      "parameters": {
        "configuration": {
          "logger": {
            "addConsoleExporter": true,
            "serviceName": "WebApp",
            "customExporters": [ "be19939a-e8ee-40e9-aea3-567a29175be8" ]
          },
          "metrics": {
            "addConsoleExporter": true,
            "meters": [ "WebApp" ],
            "customExporters": [ "be19939a-e8ee-40e9-aea3-567a29175be8" ]
          },
          "tracer": {
            "addConsoleExporter": true,
            "sources": [ "WebApp" ],
            "customExporters": [ "be19939a-e8ee-40e9-aea3-567a29175be8" ],
            "samplerType": "AlwaysOnSampler"
          },
          "perfTrackerConfiguration": {
            "meterConfig": {
              "serviceName": "WebApp",
              "serviceVersion": "1.0.0.0"
            },
            "counterConfig": {
              "label": "counter_label"
            },
            "gaugeConfig": {
              "label": "gauge_label"
            },
            "histogramConfig": {
              "label": "histogram_label",
              "unit": "ms"
            }
          }
        }
      }
    }
  ]
}